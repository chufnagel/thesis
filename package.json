{
  "name": "thesis",
  "version": "1.0.0",
  "description": "Find your adventure!",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run server",
    "server": "cross-env NODE_ENV=production node server/index.js",
    "prebuild": "rimraf dist/",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js --progress",
    "dev-watch": "cross-env NODE_ENV=development webpack --config config/webpack.dev.js -w -d",
    "start-watch": "cross-env NODE_ENV=development nodemon server/index.js",
    "dev": "npm run prebuild; concurrently \"npm:*-watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "selenium-setup": "selenium-standalone install",
    "selenium-start": "selenium-standalone start",
    "e2etests": "wdio wdio.conf.js",
    "e2etests-start": "wdio wdio.conf.js --watch",
    "test": "jest -c jest.config.json",
    "test-update": "jest -c jest.config.json --updateSnapshot",
    "test-watcher": "jest --watch",
    "test-coverage": "npm run test --coverage",
    "lint": "eslint --ext .jsx --ext .js src/",
    "lint:fix": "eslint --ext .jsx --ext .js src/ --fix",
    "lint:fix-server": "eslint server/ --fix",
    "heroku-postbuild": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoolStuffIncorporated/thesis.git"
  },
  "author": "Charles Hufnagel, Koichi Sakamaki, Lina Lei, Mandy Mak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CoolStuffIncorporated/thesis/issues"
  },
  "homepage": "https://github.com/CoolStuffIncorporated/thesis#readme",
  "devDependencies": {
